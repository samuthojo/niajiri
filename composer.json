{
    "name": "niajiri",
    "description": "Human Resource web platform for Recruitment Agency",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require":
    {
        "php": ">=7.0.0",
        "alsofronie/eloquent-uuid": "^1.0",
        "davejamesmiller/laravel-breadcrumbs": "^4.2",
        "eusonlito/laravel-meta": "^3.1",
        "felixkiss/uniquewith-validator": "^3.1",
        "fideloper/proxy": "~3.3",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravelcollective/html": "^5.5",
        "laravelista/ekko": "^1.4",
        "league/csv": "^9.0",
        "mcamara/laravel-localization": "^1.3",
        "nicolaslopezj/searchable": "^1.9",
        "owen-it/laravel-auditing": "^4.1",
        "spatie/laravel-medialibrary": "^6.1",
        "tucker-eric/eloquentfilter": "^1.2",
        "watson/rememberable": "^2.0",
        "zizaco/entrust": "^1.8"
    },
    "require-dev":
    {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload":
    {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4":
        {
            "App\\": "app/"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Tests\\": "tests/"
        }
    },
    "extra":
    {
        "laravel":
        {
            "dont-discover": []
        }
    },
    "scripts":
    {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config":
    {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
